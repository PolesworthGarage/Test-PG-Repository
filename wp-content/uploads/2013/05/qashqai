<script type="text/javascript">
<!--//--><![CDATA[//><!--
var pageDnaObj = {"content":{"authors":"","id":"59343","language":"en","has_video":false,"has_gallery":"","publish_date":"20140107","publish_time":"1637","rating_max":"10","rating_score":"9","title":"Nissan Qashqai","type":"car_review","update_date":"20140109","update_time":"1045","word_count":843},"product":{"manufacturer":"Nissan","model":"Qashqai","category":"Crossovers"},"navigation":{"locale":"en-GB","site_domain":"www.autocar.co.uk","site_name":"autocar","hierarchy":["reviews","review","intro"],"section":"reviews","section_child1":"Nissan","section_child2":"Qashqai","section_child3":"intro"},"adzone":{"2":"reviews","3":"results","4":"Nissan","5":"Qashqai"}}
//--><!]]>
