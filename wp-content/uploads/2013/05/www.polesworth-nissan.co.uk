    $(function() {
        $("#slider-range").slider({
            range: true,
            values: [ 2000, 70000 ],
            animate: true,
            min: 2000,
            max: 70000,
            step: 500,
            slide: function(event, ui) {
                if((ui.values[1] - ui.values[0]) < 1000) return false;
                $('#maxPriceDiv .value').text(ui.values[1] == 70000 ? '70,000+' : formatNumber(ui.values[1]));
                $('#minPriceDiv .value').text(formatNumber(ui.values[0]));
                $('#minPrice').val(ui.values[0])
                $('#maxPrice').val(ui.values[1])
            },
            change: function(event, ui) {
                if(renderSearchFormCompleted != false) {
                    advancedForm();
                    searchCounter();
                    trackFormStart('#frmSearch');
                }
            }
        });
        $("#slider-range a").each(function (index, domEle) {
            $(domEle).addClass('control' + index);
        });
    });
