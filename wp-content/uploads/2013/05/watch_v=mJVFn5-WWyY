      <script>
    var SnapClassList = ['content-snap-width-1', 'content-snap-width-2',
                         'content-snap-width-3'];
    var getMinWidthForGuidePinning = function(expAppbarOpenGuide) {
      if (expAppbarOpenGuide) {
        return 1251;
      }
      return 1354;
    };
    var getSnapClass = function(expAppbarOpenGuide, guidePinned) {
      var SCROLLBAR_WIDTH = 21;
      var CONTENT_PADDING = 100;
      var GUIDE_WIDTH = 230;
      var SNAP_WIDTH_2 = 1056;
      var SNAP_WIDTH_3 = 1262;
      var screenWidth = window.innerWidth ||
          document.documentElement.clientWidth;
      var minWidthForPinning = getMinWidthForGuidePinning(expAppbarOpenGuide);

      var spaceForContent = screenWidth - SCROLLBAR_WIDTH - CONTENT_PADDING;
      if (screenWidth >= minWidthForPinning && guidePinned) {
        spaceForContent -= GUIDE_WIDTH;
      }

      if (spaceForContent >= SNAP_WIDTH_3) {
        return SnapClassList[2];
      } else if (spaceForContent >= SNAP_WIDTH_2){
        return SnapClassList[1];
      } else {
        return SnapClassList[0];
      }

    };
    (function() {
      var htmlEl = document.getElementsByTagName('html')[0];
      htmlEl.className = htmlEl.className.concat(' ',
          getSnapClass(false,
              false));
    })();
  </script>

